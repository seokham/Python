player = {
  'name' : 'KARINA',
  'age' : 12,
  'alive' : True,
  'fav_food' : ("ğŸ•","ğŸ”"),
  'friend' : {
    'name' : "WINTER",
    'fav_food' : ["ğŸ"]
  }
}

player['fav_food'] = "ğŸ"
player.pop("alive")

print(player)

